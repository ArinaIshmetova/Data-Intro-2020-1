void setup() {
  size(500, 400);
  background(199,243,255);
}

void draw() {
  if (mousePressed) {
    background(199,243,255);
  } 

  stroke(0,0,0);
  fill(255,248,139);
  ellipse(200, mouseY, 100, 150);

  fill(231,186,244);
  rect(300, mouseY, 50, 300);

  fill(198,255,151);
  ellipse(mouseX, mouseY, 70, 70);
}
